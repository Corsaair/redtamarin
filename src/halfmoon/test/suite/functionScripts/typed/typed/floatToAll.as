function floatToAll() {
    var floatNumber:Number = 3.14159;
    var intNumber:int = floatNumber;
    var uintNumber:uint = floatNumber;
    var booleanNumber:Boolean = floatNumber;
    var objectNumber:Object = floatNumber;
    var stringNumber:String = floatNumber;

    print(intNumber);
    print(uintNumber);
    print(booleanNumber);
    print(objectNumber);
    print(stringNumber);
    
}

floatToAll();
